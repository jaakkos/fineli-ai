{
  "name": "fineli-ai",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.29.3",
  "scripts": {
    "dev": "next dev",
    "build": "tsx scripts/build-fineli-index.ts && next build",
    "fineli:index": "tsx scripts/build-fineli-index.ts",
    "start": "next start",
    "lint": "eslint --max-warnings 0",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:seed": "tsx src/lib/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "db:reset": "docker compose down -v && docker compose up -d && sleep 2 && drizzle-kit push",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:ui": "playwright test",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "better-sqlite3": "^12.6.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "exceljs": "^4.4.0",
    "flexsearch": "^0.8.2",
    "jose": "^6.1.3",
    "nanoid": "^5.1.6",
    "next": "16.1.6",
    "pg": "^8.18.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "resend": "^6.9.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@types/adm-zip": "^0.5.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "adm-zip": "^0.5.16",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  }
}